{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "off",
        "accessor-pairs": "off",
        "no-async-promise-executor": "off",
        "prefer-promise-reject-errors": "off",
        "no-return-assign": "off",
        "no-prototype-builtins": "off"
    }
}
